{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["react-app", "react-app/jest"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "indent": ["off"],
        "linebreak-style": ["off"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "react/react-in-jsx-scope": ["off"]
    }
}
